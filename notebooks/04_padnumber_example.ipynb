    "# 提取pdf文字\n",
    "import pdfplumber\n",
    "\n",
    "with pdfplumber.open(\"documents/吉利博越.pdf\") as pdf:\n",
    "    first_page = pdf.pages[0]\n",
    "    print(first_page.chars[0])# 获取第一页的第一个字\n",
    "    print(first_page.extract_text())"
